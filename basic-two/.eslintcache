 using image */}\r\n                <img src={netflix} style={{width:\"80px\"}}/>\r\n            </div>\r\n\r\n            {/* adding address - to */}\r\n            <Link style={{margin:\"20px 10px\"}} to=\"/home\">Home</Link>\r\n            <Link style={{margin:\"20px 10px\"}} to=\"/Item\">Item</Link>\r\n            <Link style={{margin:\"20px 10px\"}} to=\"/addedItems\">Added Items</Link>\r\n            <span style={{margin:\"20px 10px\"}}>State Value : {state}</span>\r\n            <span style={{margin:\"20px 10px\"}}>Category Value: {category}</span>\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default Navigation;","G:\\Web_Dev_Final_Projects\\react-javascript-framework\\basic-two\\src\\components\\DataLoad.js",["82","83"],"import React, { useEffect, useState, useContext } from 'react';\r\n//importing Info component\r\nimport Info from './Info';\r\n// loading the own data\r\nimport ownData from './ownData';\r\n// importing Total component\r\nimport Total from './Total';\r\n//importing database manager\r\nimport {addToDatabaseCart, getDatabaseCart} from './utilities/databaseManager';\r\n\r\nimport {CategoryContext} from '../App';\r\n\r\n\r\nimport {\r\n    Link\r\n  } from \"react-router-dom\";\r\n\r\nconst DataLoad = (props) => {\r\n\r\n    const [category,setCategory]=useContext(CategoryContext);\r\n    \r\n    const {state,setState}=props;\r\n\r\n    //selected item value saving\r\n    const [selectedItem,setSelectedItem]=useState([]);\r\n\r\n    // slicing the own data to 10 item\r\n    var loadedData=ownData.slice(0,10);\r\n\r\n    // setting the all data into use state\r\n    const [data, setdata] = useState(loadedData);\r\n\r\n\r\n    useEffect(()=>{\r\n        var savedDatas=getDatabaseCart();\r\n        const dataKeys=Object.keys(savedDatas);\r\n        const previousData=dataKeys.map(iterator=>{\r\n            const item=ownData.find(id=> id.key===iterator);\r\n            item.count=savedDatas[iterator];\r\n            return item;\r\n        })\r\n\r\n        console.log(previousData);\r\n        setSelectedItem(previousData);\r\n    },[])\r\n\r\n\r\n    // a function to send through prop\r\n    function  testFunc(item){\r\n        const itemToBeAdded=item.key;\r\n        const sameProduct=selectedItem.find(pd=> pd.key===itemToBeAdded);\r\n        let count=1;\r\n        let newItem;\r\n        if(sameProduct){\r\n            count=sameProduct.count+1;\r\n            sameProduct.count=count;\r\n            const others=selectedItem.filter(it=>it.key !== itemToBeAdded);\r\n            newItem=[...others, sameProduct];\r\n        }else{\r\n            item.count=1;\r\n            newItem=[...selectedItem,item];\r\n        }\r\n        setSelectedItem(newItem);\r\n        addToDatabaseCart(item.key,count);\r\n    }\r\n\r\n    console.log(selectedItem)\r\n\r\n    var total=selectedItem.reduce((sum,id)=>{\r\n        sum=sum+id.count;\r\n        return sum;\r\n    },0)\r\n    console.log(total);\r\n    return (\r\n        <>\r\n        <div>\r\n\r\n            <h2>Shared use-state in multiple component</h2>\r\n\r\n            <button className=\"mt-5\" onClick={()=>setState(state+1)}>Increase</button>\r\n            <button className=\"mb-5\" onClick={()=>setState(state-1)}>Decrease</button>\r\n            \r\n        </div>\r\n\r\n        <div className=\"my-4\">\r\n            <h3>Set Your Category</h3>\r\n            <button className=\"ml-2\" onClick={()=>{setCategory(\"books\")}}>Set catagory : Books</button>\r\n            <button className=\"ml-2\" onClick={()=>{setCategory(\"movies\")}}>Set category : Movies</button>\r\n            <button className=\"ml-2\" onClick={()=>{setCategory(\"\")}}>clear category</button>\r\n        </div>\r\n        \r\n        {/* keys value are passing using props  */}\r\n        <div>\r\n            {/* shared components in multiple items */}\r\n            <Total keys={total}>\r\n                <Link to=\"/addedItems\">\r\n                    <button>\r\n                        Review Items\r\n                    </button>\r\n                </Link>\r\n                \r\n            </Total>\r\n        </div>\r\n\r\n        <div>\r\n            {\r\n                // passing data from one component to another\r\n                // key must be passed\r\n                // alter: a conditional boolean render value is sent to show some button\r\n                data.map(pd=> <Info alter={true} testFunc={testFunc} key={pd.key} datas={pd} ></Info>)\r\n            }          \r\n        </div>\r\n\r\n        \r\n        </>\r\n    );\r\n};\r\n\r\nexport default DataLoad;","G:\\Web_Dev_Final_Projects\\react-javascript-framework\\basic-two\\src\\components\\ownData\\index.js",[],"G:\\Web_Dev_Final_Projects\\react-javascript-framework\\basic-two\\src\\components\\ownData\\books.js",[],"G:\\Web_Dev_Final_Projects\\react-javascript-framework\\basic-two\\src\\components\\ownData\\movies.js",[],"G:\\Web_Dev_Final_Projects\\react-javascript-framework\\basic-two\\src\\components\\Info.js",[],"G:\\Web_Dev_Final_Projects\\react-javascript-framework\\basic-two\\src\\components\\Total.js",[],"G:\\Web_Dev_Final_Projects\\react-javascript-framework\\basic-two\\src\\components\\Home.js",["84"],"import React from 'react';\r\nimport './style.css'; //importing css style file\r\nimport home from './image/home.jpg' //importing image file\r\nconst Home = () => {\r\n    return (\r\n        <>\r\n        <h3 className=\"my-3\">Welcome to Nico-flix</h3>\r\n\r\n        {/* setting image */}\r\n        <img  className=\"home-image\" src={home} />\r\n        </>\r\n    );\r\n};\r\n\r\nexport default Home;","G:\\Web_Dev_Final_Projects\\react-javascript-framework\\basic-two\\src\\components\\Notfound.js",[],"G:\\Web_Dev_Final_Projects\\react-javascript-framework\\basic-two\\src\\components\\Detail.js",["85"],"import React from 'react';\r\nimport { useParams } from 'react-router-dom';\r\nimport Info from './Info';\r\nimport ownData from './ownData';\r\n\r\nfunction Det